{
  "name": "@qc2168/vite-plugin-utools",
  "type": "module",
  "version": "1.5.1",
  "packageManager": "pnpm@8.10.2",
  "description": "quickly building utools files",
  "author": "QC2168",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/pkg-placeholder.git"
  },
  "bugs": "https://github.com/antfu/pkg-placeholder/issues",
  "keywords": [
    "utools",
    "vite"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@electron/asar": "^3.2.17",
    "cross-spawn": "^7.0.6",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/babel__traverse": "^7.20.6",
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.9",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.11",
    "pnpm": "^9.15.0",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
